pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'This stage is building the app...'
            }
        }
        stage('Test') {
            steps {
                echo 'This stage is testing the app...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'this is stage is deploying the app...'
            }
        }
    }
}

